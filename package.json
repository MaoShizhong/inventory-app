{
  "name": "inventory-app-top",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "tw": "npx tailwindcss -i ./dist/public/stylesheets/tailwind.css -o ./dist/public/stylesheets/styles.css --watch",
    "start": "node ./dist/bin/www",
    "devstart": "nodemon ./dist/bin/www",
    "serverstart": "concurrently -k -n COMPILER,NODEMON -c blueBright,magenta \"npx tsc --watch\" \"npm run tw\" \"DEBUG=inventory-app-TOP:* npm run devstart\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-errors": "~1.6.3",
    "mongoose": "^7.5.0",
    "morgan": "~1.9.1",
    "postcss-cli": "^10.1.0",
    "pug": "2.0.0-beta11",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
}